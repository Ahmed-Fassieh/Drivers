#ifndef EXT_Int_H
#define EXT_Int_H


#define MCUCR *((volatile u8*)0x55)

#define MCUCR_ISC11	3
#define MCUCR_ISC10	2
#define MCUCR_ISC01	1
#define MCUCR_ISC00	0

#define MWSR *((volatile u8*)0x54)

#define MWSR_ISC2	6

#define GICR_INT1 *((volatile u8*)0x5B)

#define GICR_INT1	7
#define GICR_INT0	6
#define GICR_INT2	5

#define GIFR *((volatile u8*)0x5A)


#endif